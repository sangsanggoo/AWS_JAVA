    "def my_sq_prod(number_list):\n",
    "   return_value = 1;\n",
    "   for number in number_list:\n",
    "        return_value = return_value * number**2\n",
    "   return return_value\n",
    "assert my_sq_prod([1,2,3]) == 36"
   ]
  }
