    "import numpy as np\n",
    "def fact_r(r) :\n",
    "    result = 1\n",
    "    while(1<r) :\n",
    "        result *= r\n",
    "        r= r-1\n",
    "    return result\n",
    "        \n",
    "def perm(n,r) :\n",
    "    return fact_r(n) / fact_r(n-r)\n",
    "print(fact_r(4))"
   ]
